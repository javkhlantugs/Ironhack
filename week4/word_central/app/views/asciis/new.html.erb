<h1>Word Central</h1>

<h2>Submit text for inspection</h2>

<form method="post" action="/asciis">
  <div>
    <label for="text_inspection_user_text">Text</label>
    <textarea name="text_inspection[user_text]" id="text_inspection_user_text"></textarea>
  </div>
  <button type="submit">Submit</button>
  <input type="hidden" name="authenticity_token"
      value="<%= form_authenticity_token %>">
</form>